{
	"name": "server",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"start:build": "nest build",
		"start:format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start:start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"start:lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"start:test": "jest",
		"start:test:watch": "jest --watch",
		"start:test:cov": "jest --coverage",
		"start:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"start:test:e2e": "jest --config ./test/jest-e2e.json"
	},
	"dependencies": {
		"@nestjs/common": "^10.3.8",
		"@nestjs/config": "^3.2.2",
		"@nestjs/core": "^10.3.8",
		"@nestjs/mongoose": "^10.0.6",
		"@nestjs/platform-express": "^10.3.8",
		"@nestjs/swagger": "^7.3.1",
		"@nestjs/throttler": "^5.1.2",
		"body-parser": "^1.20.2",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.1",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"helmet": "^7.1.0",
		"mongodb": "^6.5.0",
		"mongoose": "^8.3.2",
		"nestjs-rate-limiter": "^3.1.0",
		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.1"
	},
	"devDependencies": {
		"@nestjs/cli": "^10.3.2",
		"@nestjs/schematics": "^10.1.1",
		"@nestjs/testing": "^10.3.8",
		"@tsconfig/node20": "^20.1.4",
		"@types/body-parser": "^1.19.5",
		"@types/compression": "^1.7.5",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/jest": "^29.5.12",
		"@types/node": "^20.12.7",
		"@types/supertest": "^6.0.2",
		"@types/toobusy-js": "^0.5.4",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-security": "^3.0.0",
		"jest": "^29.7.0",
		"nodemon": "^3.1.0",
		"prettier": "^3.2.5",
		"source-map-support": "^0.5.21",
		"supertest": "^6.3.4",
		"ts-jest": "^29.1.2",
		"ts-loader": "^9.5.1",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.4.5"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
